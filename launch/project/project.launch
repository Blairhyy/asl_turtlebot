<launch>
  <arg name="sim" default="true"/>
  <rosparam param="use_tf">true</rosparam>
  <rosparam param="explore">true</rosparam>

  <include file="$(find asl_turtlebot)/launch/root.launch">
    <arg name="world" value="project_city"/>
    <arg name="x_pos" default="3.15"/>
    <arg name="y_pos" default="1.6"/>
    <arg name="z_pos" default="0.0"/>
    <arg name="rviz" default="final_project"/>
    <arg name="model" default="asl_turtlebot"/>
    <arg name="sim" default="$(arg sim)"/>
  </include>
<node pkg="asl_turtlebot" type="detector_mobilenet.py" name="turtlebot_detector" output="screen" />
<!-- <node pkg="asl_turtlebot" type="navigator.py" name="turtlebot_navigator" output="screen" /> -->
<!-- <node pkg="asl_turtlebot" type="pub_Marker.py" name="goal_viz_node" output="screen" /> -->
</launch>
